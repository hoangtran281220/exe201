@page
@model exeFinal.Pages.SetClotherModel
@{
    ViewData["Title"] = "Set đồ";
    Layout = "_Layout";
}

@* Header + Navigation *@
@* @await Html.PartialAsync("_TopBar") *@
@await Html.PartialAsync("_Header")
@await Html.PartialAsync("_MainNav")

<div class="container">
    <div class="content">
        @await Html.PartialAsync("_Sidebar")

        <main class="main">
            <h3>SET ĐỒ</h3>

            @* Nếu bạn có danh sách sản phẩm, đặt thêm ở đây *@
            <div class="product-grid">
                <div class="product-item">
                    <img src="/images/shin of.png" alt="clother of shin">
                    <h3>Set đồ shinichi 1</h3>
                    <p class="price">49.000</p>
                    <a href="https://www.facebook.com/profile.php?id=61576621782744" target="_blank" class="btn">MUA NGAY</a>
                </div>
                <div class="product-item">
                    <img src="/images/ram.png" alt="ran">
                    <h3>Set đồ ran 1</h3>
                    <p class="price">49.000</p>
                    <a href="https://www.facebook.com/profile.php?id=61576621782744" target="_blank" class="btn">MUA NGAY</a>
                </div>
            </div>
        </main>
    </div>
</div>

@await Html.PartialAsync("_PreFooter")
@await Html.PartialAsync("_MainFooter")
